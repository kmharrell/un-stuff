  line-height: 28px;
}
#block-block-15 {
  background: #e1e8f2;
  padding: 20px 0;
}
#block-block-15 p{
  margin: 0px;
}

#block-block-20 {
  background: #F4F7FA;
  width: 100%;
}
#block-views-announcement-block {
color: #B94A48;
background-color: #F2DEDE;
border: 1px solid #EED3D7;
width: 100%;
}
#block-views-announcement-block .views-field-title a {
color: #B94A48;
text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
padding: 8px 35px 8px 14px;
